@model IEnumerable<WebStore.Models.Employee>

<h3>Сотрудники:</h3>
<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Возраст</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.LastName</td>
                <td>@employee.FirstName</td>
                <td>@employee.Patronymic</td>
                <td>@employee.Age</td>
                <td>
                    <a asp-action="Details" asp-route-id="@employee.Id">
                        <i class="fa fa-info-circle"></i>
                    </a>|<a asp-action="Edit" asp-route-id="@employee.Id">
                        <i class="fa fa-edit"></i>
                    </a>|<a asp-action="Delete" asp-route-id="@employee.Id">
                        <i class="fa fa-trash"></i>
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>
<hr />
<a asp-action="Create" class="btn btn-success">Создать</a>